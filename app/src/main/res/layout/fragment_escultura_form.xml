<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <EditText
        android:id="@+id/editTextDescripcionEscultura"
        style="@style/textField"
        android:layout_width="323dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:ems="10"
        android:hint="Descripción"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="57dp"
        android:layout_marginBottom="28dp"
        android:text="cm"
        app:layout_constraintBottom_toTopOf="@+id/editTextAutorEscultura"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextMaterialEscultura" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginBottom="29dp"
        android:text="Kg"
        app:layout_constraintBottom_toTopOf="@+id/editTextAutorEscultura"
        app:layout_constraintEnd_toStartOf="@+id/editTextAlturaEscultura"
        app:layout_constraintHorizontal_bias="0.281"
        app:layout_constraintStart_toEndOf="@+id/editTextPesoEscultura"
        app:layout_constraintTop_toBottomOf="@+id/editTextMaterialEscultura" />

    <EditText
        android:id="@+id/editTextAlturaEscultura"
        style="@style/textField"
        android:layout_width="87dp"
        android:layout_height="55dp"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="23dp"
        android:ems="10"
        android:hint="Altura"
        android:inputType="text"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextAutorEscultura"
        app:layout_constraintEnd_toStartOf="@+id/editTextAnioEscultura"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextMaterialEscultura" />

    <EditText
        android:id="@+id/editTextAnchuraEscultura2"
        style="@style/textField"
        android:layout_width="83dp"
        android:layout_height="49dp"
        android:layout_marginTop="172dp"
        android:ems="10"
        android:hint="Anchura"
        android:inputType="text"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.823"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextAnioEscultura" />

    <EditText
        android:id="@+id/editTextPesoEscultura"
        style="@style/textField"
        android:layout_width="59dp"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="28dp"
        android:ems="10"
        android:hint="Peso"
        android:inputType="text"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextAutorEscultura"
        app:layout_constraintEnd_toStartOf="@+id/editTextAlturaEscultura"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextMaterialEscultura" />

    <EditText
        android:id="@+id/editTextCorrienteEscultura"
        style="@style/textField"
        android:layout_width="323dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="416dp"
        android:ems="10"
        android:hint="Corriente Artisitica"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/editTextMaterialEscultura"
        style="@style/textField"
        android:layout_width="323dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="188dp"
        android:ems="10"
        android:hint="Material"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/editTextAutorEscultura"
        style="@style/textField"
        android:layout_width="323dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="340dp"
        android:ems="10"
        android:hint="Autor"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="62dp"
        android:layout_marginBottom="23dp"
        android:text="cm"
        app:layout_constraintBottom_toTopOf="@+id/editTextAutorEscultura"
        app:layout_constraintEnd_toStartOf="@+id/editTextAnchuraEscultura2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/editTextAlturaEscultura"
        app:layout_constraintTop_toBottomOf="@+id/editTextMaterialEscultura" />

    <EditText
        android:id="@+id/editTextNomEscultura"
        style="@style/textField"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="text"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextDescripcionEscultura"
        app:layout_constraintEnd_toStartOf="@+id/editTextAnioEscultura"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/editTextAnioEscultura"
        style="@style/textField"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:hint="Año"
        android:inputType="text"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextDescripcionEscultura"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Agregar Escultura"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinnerSitioEsculturaForm"
        android:layout_width="323dp"
        android:layout_height="50dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/botonRegistrarEscultura"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextCorrienteEscultura"
        app:layout_constraintVertical_bias="0.295" />

    <Button
        android:id="@+id/botonRegistrarEscultura"
        android:layout_width="141dp"
        android:layout_height="48dp"
        android:text="Registrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextCorrienteEscultura"
        app:layout_constraintVertical_bias="0.66" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sitio"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.714" />
</androidx.constraintlayout.widget.ConstraintLayout>